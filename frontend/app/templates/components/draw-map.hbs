<div class="devel-visible">
  {{!-- This info will be thinned or removed as axes development progresses. --}}
  <div>splitAxes : {{splitAxes}}</div>
  <div>axes : {{axes.length}}</div>
  {{#each axes as |axisS|}}
    <div>axis : {{axisS}}, {{axisS.apID}}, {{axisS.extended}}</div>
    {{axis-2d drawMap=this data=oa apID=axisS.apID tableData=axisData}}
  {{/each}}
</div>

