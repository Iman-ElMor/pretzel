{{record/entry-record
  entry=entry}}

<h5> {{elem/icon-base name="arrow-up"}}
  <span class="label label-info">
    {{elem/icon-base name="asterisk"}} {{entry.features.[0]}} {{entry.positions.[0]}}
  </span>
</h5>
<h5> {{elem/icon-base name="arrow-down"}}
  <span class="label label-info">
    {{elem/icon-base name="asterisk"}} {{entry.features.[1]}} {{entry.positions.[1]}}
  </span>
</h5>
