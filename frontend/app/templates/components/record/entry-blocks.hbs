<ul>
  {{#each data as |block|}}
    <li class="list-group-item list-group-item {{if (compare block.id '===' selectedBlock.id) 'list-group-item-warning'}}" {{action "selectBlockAndDataset" block on="click"}} style="display: inline-block;">
      {{record/entry-block-by-scope
        loadBlock="loadBlock"
        entry=block }}
      {{!--
      {{block.datasetId.name}}
      <span class="blockInScope">{{block.name}}</span>
      --}}
    </li>
  {{/each}}
</ul>
