<ul>
  {{#each data as |block|}}
    <li class="list-group-item list-group-item-info" style="display: inline-block;" >
      {{record/entry-block-by-scope
        loadBlock="loadBlock"
        entry=block }}
      {{!--
      {{block.datasetId.name}}
      <span class="blockInScope">{{block.name}}</span>
      --}}
    </li>
  {{/each}}
</ul>
