{{log 'entry-values' values valueIsArray valueIsDatasetsArray valueIsBlocksArray valuesIsMap}}

{{#if valueIsDatasetsArray}}   {{!-- type is blocks array --}}
  {{log 'entry-datasets' values}}
  {{record/entry-datasets
    loadBlock="loadBlock"
    selectBlock=(action 'selectBlock')
    selectedBlock=selectedBlock
    selectDataset=(action 'selectDataset')
    selectedDataset=selectedDataset
    data=values}}

{{else if valueIsBlocksArray }}   {{!-- type is blocks array --}}
      {{log 'entry-blocks'}}
      {{record/entry-blocks
        loadBlock="loadBlock"
        data=values
        selectBlock="selectBlock"
        selectedBlock=selectedBlock
        selectDataset="selectDataset"}}

{{else if valuesIsMap}}
  {{log 'valuesIsMap:each'}}

  {{#each values as |key value|}}
    {{log  'value' value 'key' key 'levelComponent' levelComponent}}
    {{component levelComponent levelMeta=levelMeta name=key values=value
      loadBlock="loadBlock"
      selectBlock="selectBlock"
      selectedBlock=selectedBlock
      selectDataset="selectDataset"
    }}
  {{/each}}

{{else}}
  {{log 'each-in'}}

  {{#each-in values as |key value|}}
    {{log  'key' key 'value' value 'levelComponent' levelComponent}}
    {{component levelComponent levelMeta=levelMeta name=key values=value
      loadBlock="loadBlock"
      selectBlock="selectBlock"
      selectedBlock=selectedBlock
      selectDataset="selectDataset"
    }}
  {{/each-in}}

{{/if}}
