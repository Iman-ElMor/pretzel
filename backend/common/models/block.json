{
  "name": "Block",
  "base": "PersistedModel",
  "description": "Data block that contains features",
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "dataset": {
      "type": "belongsTo",
      "model": "Dataset",
      "foreignKey": ""
    },
    "features": {
      "type": "hasMany",
      "model": "Feature",
      "foreignKey": ""
    },
    "annotations": {
      "type": "hasMany",
      "model": "Annotation",
      "foreignKey": ""
    },
    "intervals": {
      "type": "hasMany",
      "model": "Interval",
      "foreignKey": ""
    }
  },
  "acls": [
  ],
  "methods": {},
  "scope": {
    "include": ["annotations", "intervals"]
  }
}
